{
    "configurations": [
        {
            "name": "Linux-CrossCompile-Windows-32bit",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/i686-w64-mingw32/include/**",
                "/usr/share/mingw-w64/include/**"
            ],
            "defines": [
                "_DEBUG",
                "WIN32",
                "_WIN32_WINNT=0x0601",
                "__MINGW32__"
            ],
            "compilerPath": "/usr/bin/i686-w64-mingw32-g++",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x86"
        }
    ],
    "version": 4
}
